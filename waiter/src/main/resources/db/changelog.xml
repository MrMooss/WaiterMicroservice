<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                   http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">

    <changeSet id="1" author="Atimo">
        <createTable tableName="order">
            <column name="id" type="BIGINT" autoIncrement="true">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="asztal_id" type="BIGINT">
                <constraints nullable="false"/>
            </column>
            <column name="group" type="VARCHAR(255)">
                <constraints nullable="true"/>
            </column>
            <column name="menu_id" type="BIGINT">
                <constraints nullable="false"/>
            </column>
            <column name="menu_neve" type="VARCHAR(255)">
                <constraints nullable="false"/>
            </column>
            <column name="desc" type="VARCHAR(255)">
                <constraints nullable="true"/>
            </column>
            <column name="db" type="TINYINT">
                <constraints nullable="false"/>
            </column>
            <column name="status" type="VARCHAR(255)">
                <constraints nullable="false"/>
            </column>
            <column name="hely" type="VARCHAR(255)">
                <constraints nullable="true"/>
            </column>
            <column name="egyseg_ar" type="DOUBLE">
                <constraints nullable="false"/>
            </column>
            <column name="osszes_ar" type="DOUBLE">
                <constraints nullable="false"/>
            </column>
            <column name="created_time_stamp" type="TIMESTAMP">
                <constraints nullable="false"/>
            </column>
            <column name="modified_time_stamp" type="TIMESTAMP">
                <constraints nullable="false"/>
            </column>
        </createTable>
    </changeSet>

</databaseChangeLog>
